<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Push image nikolayp/clock-web:ubuntu.20.04</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="DEPLOYMENT" />
    </options>
    <parameters />
    <build-runners>
      <runner id="RUNNER_1" name="Pull or update mcr.microsoft.com/dotnet/runtime-deps:5.0-focal" type="DockerCommand">
        <parameters>
          <param name="command.args" value="mcr.microsoft.com/dotnet/runtime-deps:5.0-focal" />
          <param name="docker.command.type" value="other" />
          <param name="docker.push.remove.image" value="true" />
          <param name="docker.sub.command" value="pull" />
          <param name="dockerfile.source" value="PATH" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_2" name="Build clock-web:ubuntu.20.04" type="DockerCommand">
        <parameters>
          <param name="command.args" value="--build-arg baseImage=mcr.microsoft.com/dotnet/runtime-deps:5.0-focal" />
          <param name="docker.command.type" value="build" />
          <param name="docker.image.namesAndTags" value="clock-web:ubuntu.20.04" />
          <param name="docker.push.remove.image" value="true" />
          <param name="dockerImage.platform" value="linux" />
          <param name="dockerfile.contextDir" value="context" />
          <param name="dockerfile.path" value="context/web.linux.dockerfile" />
          <param name="dockerfile.source" value="PATH" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_3" name="Re-tag nikolayp/clock-web:ubuntu.20.04" type="DockerCommand">
        <parameters>
          <param name="command.args" value="clock-web:ubuntu.20.04 nikolayp/clock-web:ubuntu.20.04" />
          <param name="docker.command.type" value="other" />
          <param name="docker.push.remove.image" value="true" />
          <param name="docker.sub.command" value="tag" />
          <param name="dockerfile.source" value="PATH" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
      <runner id="RUNNER_4" name="Push nikolayp/clock-web:ubuntu.20.04 to Docker Hub" type="DockerCommand">
        <parameters>
          <param name="docker.command.type" value="push" />
          <param name="docker.image.namesAndTags" value="nikolayp/clock-web:ubuntu.20.04" />
          <param name="docker.push.remove.image" value="true" />
          <param name="dockerfile.source" value="PATH" />
          <param name="teamcity.step.mode" value="default" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings>
      <vcs-entry-ref root-id="TeamcityDotnetSamples2_SamplesRepo" />
    </vcs-settings>
    <requirements>
      <starts-with id="RQ_1" name="teamcity.agent.jvm.os.name" value="Linux" />
    </requirements>
    <build-triggers />
    <build-extensions>
      <extension id="BUILD_EXT_1" type="DockerSupport">
        <parameters>
          <param name="cleanupPushed" value="true" />
          <param name="login2registry" value="PROJECT_EXT_728" />
          <param name="loginCheckbox" value="on" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="TeamcityDotnetSamples2_BuildConsoleAndWebLinux64" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="bin =&gt; context" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="TeamcityDotnetSamples2_BuildConsoleAndWebLinux64">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

