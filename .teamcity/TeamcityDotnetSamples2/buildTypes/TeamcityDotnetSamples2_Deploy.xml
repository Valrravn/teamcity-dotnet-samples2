<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>Deploy</name>
  <description />
  <settings>
    <options>
      <option name="buildConfigurationType" value="DEPLOYMENT" />
    </options>
    <parameters />
    <build-runners />
    <vcs-settings />
    <requirements />
    <build-triggers />
    <dependencies>
      <depend-on sourceBuildTypeId="TeamcityDotnetSamples2_PushConsoleMultiArch">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="TeamcityDotnetSamples2_PushWebMultiArch">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
      <depend-on sourceBuildTypeId="TeamcityDotnetSamples2_PublishToNuget">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

